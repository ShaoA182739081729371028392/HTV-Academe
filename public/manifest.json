{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "tabCapture",
        "downloads",
        "storage",
        "scripting",
        "contextMenus",
        "notifications"
    ],
    "icons": {
        "16": "icons/16.png"
    },
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service-worker": "background.js"
    }
  }